sap.ui.define([                                                                                                                                                                                                                                                
    "sap/ui/core/mvc/Controller"                                                                                                                                                                                                                               
],                                                                                                                                                                                                                                                             
    /**                                                                                                                                                                                                                                                        
     * @param {typeof sap.ui.core.mvc.Controller} Controller                                                                                                                                                                                                   
     */                                                                                                                                                                                                                                                        
    function (Controller) {                                                                                                                                                                                                                                    
        "use strict";                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
        return Controller.extend("sync.zca01nav0102.controller.View1", {                                                                                                                                                                                       
            onInit: function () {                                                                                                                                                                                                                              
                //서비스 생성?                                                                                                                                                                                                                                      
                this._fnGetService = sap.ushell && sap.ushell.Container && sap.ushell.Container.getService;                                                                                                                                                    
                //네비게이션 설정                                                                                                                                                                                                                                     
                this._oCrossAppNavigation = this._fnGetService && this._fnGetService("CrossApplicationNavigation");                                                                                                                                            
           },                                                                                                                                                                                                                                                  
           onTableItemClick: function ( oEvent ) {                                                                                                                                                                                                             
               let sContext = oEvent.getSource().getBindingContext();                                                                                                                                                                                          
               let carrid = sContext.getProperty( "Carrid" );                                                                                                                                                                                                  
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                     
               // Move New App                                                                                                                                                                                                                                 
               if( this._oCrossAppNavigation ) {                                                                                                                                                                                                               
                   let oHref = this._oCrossAppNavigation.toExternal({                                                                                                                                                                                          
                       target : {                                                                                                                                                                                                                              
                           semanticObject : "CLA01",                                                                                                                                                                                                           
                           action: "navto"                                                                                                                                                                                                                     
                       },                                                                                                                                                                                                                                      
                       params : {                                                                                                                                                                                                                              
                           "pCarrid" : carrid                                                                                                                                                                                                                  
                       }                                                                                                                                                                                                                                       
                   });                                                                                                                                                                                                                                         
               }                                                                                                                                                                                                                                               
           }                                                                                                                                                                                                                                                   
        });                                                                                                                                                                                                                                                    
    });                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               